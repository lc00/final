extends layout

block content
	div.container.margin-top
		div.row
			div.col-md-3.text-center
				h3 Level of Difficulty

		- for (var i = 1; i < 10; i++)
			div.row
				div.col-md-3.text-center
					h4 #{i}
		div.row
			div.col-md-3.text-center
				h3 Category of Shots
		div.row
			div.col-md-3.text-center
				h4 8 ball
				h4 9 ball
				h4 10 ball
				h4 Straight pool
				h4 Single shots
				h4 Multiple Shots
				h4 Position shots